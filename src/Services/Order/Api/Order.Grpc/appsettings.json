{
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Serilog": {
      "Enable": true,
      "ServiceName": "order.grpc.server",
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "System": "Information"
        }
      },
      "Console": {
        "Enable": true,
        "Level": "Debug"
      },
      "Otlp": {
        "Endpoint": "http://otel-collector:4317"
      }
    }
  },
  "ConnectionStrings": {
    "DbType": "SQLSERVER",
    "Database": "Server=sql-server,1433;Database=Order_Service;User Id=sa;Password=123456789Aa;Encrypt=False;TrustServerCertificate=True;"
  },
  "MessageBroker": {
    "Host": "amqp://rabbitmq:5672",
    "UserName": "admin",
    "Password": "123456789Aa",
    "Port": 5672
  },
  "AppConfig": {
    "ServiceName": "order.grpc.server",
    "ApiKey": "progcoder.com",
    "GrpcApiKey": "progcoder.com",
    "IncludeInnerException": true,
    "IncludeExceptionStackTrace": true
  },
  "DistributedTracing": {
    "Enable": true,
    "ServiceName": "order.grpc.server",
    "Source": "order.grpc.server.diagnostics",
    "SamplingRate": 0.25,
    "Prometheus": {
      "Enable": true
    },
    "Otlp": {
      "Endpoint": "http://otel-collector:4317",
      "TimeoutMs": 10000
    }
  },
  "GrpcClients": {
    "Catalog": {
      "Url": "http://catalog-grpc:8080",
      "ApiKey": "progcoder.com"
    },
    "Discount": {
      "Url": "http://discount-grpc:8080",
      "ApiKey": "progcoder.com"
    }
  }
}
