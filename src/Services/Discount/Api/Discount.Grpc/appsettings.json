{
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Serilog": {
      "Enable": true,
      "ServiceName": "discount.grpc.server",
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "System": "Information"
        }
      },
      "Console": {
        "Enable": true,
        "Level": "Debug"
      },
      "Otlp": {
        "Endpoint": "http://localhost:4317"
      }
    }
  },
  "ConnectionStrings": {
    "DbType": "MONGODB",
    "DatabaseName": "Discount_Service",
    "Database": "mongodb://mongodb:123456789Aa@localhost:27017"
  },
  "MessageBroker": {
    "Host": "amqp://localhost:5672",
    "UserName": "admin",
    "Password": "123456789Aa",
    "Port": 5672
  },
  "MinIO": {
    "Endpoint": "localhost:9000",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "Secure": false
  },
  "AppConfig": {
    "ServiceName": "discount.grpc.server",
    "ApiKey": "progcoder.com",
    "GrpcApiKey": "progcoder.com",
    "IncludeInnerException": true,
    "IncludeExceptionStackTrace": true
  },
  "ApiClients": {
    "Keycloak": {
      "BaseUrl": "http://localhost:8080",
      "Realm": "prog-coder-realm",
      "ClientId": "prog-coder-service-account",
      "ClientSecret": "HvK7fa4wOn9e5wKpxJA2tyd5SK5oGy8c",
      "Scopes": [ "openid" ],
      "GrantType": "client_credentials"
    }
  },
  "DistributedTracing": {
    "Enable": true,
    "ServiceName": "discount.grpc.server",
    "Source": "discount.grpc.server.diagnostics",
    "SamplingRate": 0.25,
    "Prometheus": {
      "Enable": true
    },
    "Otlp": {
      "Endpoint": "http://localhost:4317",
      "TimeoutMs": 10000
    }
  }
}
