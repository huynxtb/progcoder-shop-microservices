{
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Serilog": {
      "Enable": true,
      "ServiceName": "inventory.grpc.server",
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "System": "Information"
        }
      },
      "Console": {
        "Enable": true,
        "Level": "Debug"
      },
      "Otlp": {
        "Endpoint": "http://otel-collector:4317"
      }
    }
  },
  "ConnectionStrings": {
    "DbType": "MYSQL",
    "Database": "Server=mysql;Port=3306;Database=Inventory_Service;Uid=root;Pwd=123456789Aa;"
  },
  "MessageBroker": {
    "Host": "amqp://rabbitmq:5672",
    "UserName": "admin",
    "Password": "123456789Aa",
    "Port": 5672
  },
  "AppConfig": {
    "ServiceName": "inventory.grpc.server",
    "ApiKey": "progcoder.com",
    "GrpcApiKey": "progcoder.com",
    "IncludeInnerException": true,
    "IncludeExceptionStackTrace": true
  },
  "ApiClients": {
    "Keycloak": {
      "BaseUrl": "http://keycloak:8080",
      "Realm": "prog-coder-realm",
      "ClientId": "prog-coder-service-account",
      "ClientSecret": "HvK7fa4wOn9e5wKpxJA2tyd5SK5oGy8c",
      "Scopes": [ "openid" ],
      "GrantType": "client_credentials"
    }
  },
  "DistributedTracing": {
    "Enable": true,
    "ServiceName": "inventory.grpc.server",
    "Source": "inventory.grpc.server.diagnostics",
    "SamplingRate": 0.25,
    "Prometheus": {
      "Enable": true
    },
    "Otlp": {
      "Endpoint": "http://otel-collector:4317",
      "TimeoutMs": 10000
    }
  },
  "GrpcClients": {
    "Catalog": {
      "Url": "http://catalog-grpc:8080",
      "ApiKey": "progcoder.com"
    }
  }
}
