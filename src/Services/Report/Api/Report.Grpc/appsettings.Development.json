{
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Serilog": {
      "Enable": true,
      "ServiceName": "report.grpc.server",
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "System": "Information"
        }
      },
      "Console": {
        "Enable": true,
        "Level": "Debug"
      },
      "Otlp": {
        "Endpoint": "http://localhost:4317"
      }
    }
  },
  "ConnectionStrings": {
    "DbType": "MONGODB",
    "DatabaseName": "Report_Service",
    "Database": "mongodb://mongodb:123456789Aa@localhost:27017"
  },
  "AppConfig": {
    "ServiceName": "report.grpc.server",
    "ApiKey": "progcoder.com",
    "GrpcApiKey": "progcoder.com",
    "IncludeInnerException": true,
    "IncludeExceptionStackTrace": true
  },
  "DistributedTracing": {
    "Enable": true,
    "ServiceName": "report.grpc.server",
    "Source": "report.grpc.server.diagnostics",
    "SamplingRate": 0.25,
    "Prometheus": {
      "Enable": true
    },
    "Otlp": {
      "Endpoint": "http://localhost:4317",
      "TimeoutMs": 10000
    }
  }
}
